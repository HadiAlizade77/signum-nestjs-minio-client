{
  "name": "nestjs-minio-client",
  "version": "1.1.0",
  "description": "Minio module for NestJS framework",
  "author": {
    "name": "Uros Karic",
    "email": "hello@uroskaric.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djedlajn/nestjs-minio-client"
  },
  "keywords": [
    "nestjs",
    "minio",
    "minio-client-sdk"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && npm run build:index",
    "build:index": "rimraf ./index.js ./index.d.ts && tsc -d --skipLibCheck ./index.ts",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "prepublishOnly": "npm run format && npm run build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "minio": "^7.0.25",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  },
  "resolutions": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/testing": "^8.2.3",
    "@types/jest": "^27.0.3",
    "@types/minio": "^7.0.11",
    "@types/node": "^17.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
